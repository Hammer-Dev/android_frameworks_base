<resources xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- label for item that screenshots in phone options dialog -->
    <string name="global_action_screenshot">스크린샷</string>

    <!-- label for item that toggles expand desktop mode -->
    <string name="global_actions_toggle_expanded_desktop_mode">확장 데스크탑</string>
    <!-- status message in phone options dialog for when expand desktop mode is on -->
    <string name="global_actions_expanded_desktop_mode_on_status">활성화</string>
    <!-- status message in phone options dialog for when expand desktop mode is off -->
    <string name="global_actions_expanded_desktop_mode_off_status">비활성화</string>
    <!-- Button to reboot the phone, within the Phone Options dialog -->
    <string name="reboot_system" product="tablet">디바이스 재부팅</string>
    <string name="reboot_system" product="default">디바이스 재부팅</string>
    <!-- Button to reboot the phone, within the Reboot Options dialog -->
    <string name="reboot_reboot">재부팅</string>
    <!-- Button to reboot the phone into recovery, within the Reboot Options dialog -->
    <string name="reboot_recovery">리커버리 모드</string>
    <!-- Button to reboot the phone into bootloader, within the Reboot Options dialog -->
    <string name="reboot_bootloader">다운로드 모드</string>
    <!-- Button to reboot the phone into bootmenu, within the Reboot Options dialog -->
    <string name="reboot_bootmenu">부팅 메뉴</string>
    <!-- Button to reboot the phone into fastboot, within the Reboot Options dialog -->
    <string name="reboot_fastboot">패스트 부트</string>
    <!-- Button to reboot the phone into download, within the Reboot Options dialog -->
    <string name="reboot_download">다운로드 모드</string>

    <!-- Reboot Progress Dialog. This is shown if the user chooses to reboot the phone. -->
    <string name="reboot_progress">재부팅\u2026</string>
    <!-- Reboot Confirmation Dialog.  When the user chooses to reboot the phone, there will be a confirmation dialog.  This is the message. -->
    <string name="reboot_confirm" product="tablet">디바이스가 재부팅됩니다.</string>
    <string name="reboot_confirm" product="default">디바이스가 재부팅됩니다.</string>
</resources>
